Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"Helper", " ", "functions"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"ClearAll", "[", 
    RowBox[{"sp", ",", "cpols", ",", "gamma", ",", "gam", ",", "slash"}], 
    "]"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"sp", "[", 
     RowBox[{"a_", ",", "b_"}], "]"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{"a", "[", 
       RowBox[{"[", "1", "]"}], "]"}], "*", 
      RowBox[{"b", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], "-", 
     RowBox[{
      RowBox[{"a", "[", 
       RowBox[{"[", 
        RowBox[{"2", ";;"}], "]"}], "]"}], ".", 
      RowBox[{"b", "[", 
       RowBox[{"[", 
        RowBox[{"2", ";;"}], "]"}], "]"}]}]}]}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"cpols", "[", "mom_", "]"}], ":=", 
    RowBox[{"Block", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"epp1", ",", "epp2", ",", "polVec"}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"epp1", "=", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"mom", "[", 
            RowBox[{"[", 
             RowBox[{"-", "1"}], "]"}], "]"}], ">", "0"}], ",", 
          RowBox[{"{", 
           RowBox[{"0", ",", 
            FractionBox[
             RowBox[{"mom", "[", 
              RowBox[{"[", 
               RowBox[{"-", "1"}], "]"}], "]"}], 
             SqrtBox[
              SuperscriptBox[
               RowBox[{"mom", "[", 
                RowBox[{"[", 
                 RowBox[{"-", "1"}], "]"}], "]"}], "2"]]], ",", "0", ",", 
            "0"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"0", ",", 
            RowBox[{"-", 
             FractionBox[
              RowBox[{"mom", "[", 
               RowBox[{"[", 
                RowBox[{"-", "1"}], "]"}], "]"}], 
              SqrtBox[
               SuperscriptBox[
                RowBox[{"mom", "[", 
                 RowBox[{"[", 
                  RowBox[{"-", "1"}], "]"}], "]"}], "2"]]]}], ",", "0", ",", 
            "0"}], "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"epp2", "=", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"mom", "[", 
            RowBox[{"[", 
             RowBox[{"-", "1"}], "]"}], "]"}], ">", "0"}], ",", 
          RowBox[{"{", 
           RowBox[{"0", ",", "0", ",", "1", ",", "0"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"0", ",", "0", ",", 
            RowBox[{"-", "1"}], ",", "0"}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"polVec", "=", 
        RowBox[{"Conjugate", "/@", 
         RowBox[{"{", 
          RowBox[{
